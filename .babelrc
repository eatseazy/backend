{
  "presets": [
    [ "@babel/preset-env",
      {
        "useBuiltIns": "usage",
        "corejs": 3
      }
    ]
  ],
  "plugins": [
    ["module-resolver", {
      "root": ["./src"],
      "alias": {
        "lib": "./src/lib",
        "@services": "./src/app/services",
        "@models": "./src/app/models",
        "@modules": "./src/app/modules"
      }
    }]
  ]
}
